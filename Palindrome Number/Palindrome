class Solution {
    public boolean isPalindrome(int x) {
        boolean res;
        String x1=String.valueOf(x);
        String x2="";
        for(int i=0;i<x1.length();i++)
        {
            x2=x1.charAt(i)+x2;
        }
        if(x1.equals(x2))
        {
            res=true;
        }
        else
        {
            res=false;
        }
        return res;
    }
    
}
